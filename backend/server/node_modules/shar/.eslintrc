{
    "rules": {
        "no-unused-expressions": [0],
        "no-underscore-dangle":  [0],
        "no-reserved-keys":      [2],
        "no-process-exit":       [0],
        "no-multi-spaces":       [0],
        "no-extra-parens":       [2],
        "no-unused-vars":        [2],
        "no-loop-func":          [0],
        "key-spacing":           [0],
        "no-console":            [2],
        "no-empty":              [0],
        "new-cap":               [0],
        "max-len":               [2, 80],
        "strict":                [0],
        "indent":                [4],
        "quotes":                [2, "single", "avoid-escape"],
        "curly":                 [0]
    },
    "env": {
        "browser": true,
        "node": true
    }
}
